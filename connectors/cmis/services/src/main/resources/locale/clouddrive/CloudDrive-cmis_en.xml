<bundle>
  <!-- Used in Administration-Content-Explorer in Views settings a popup with actions list -->
  <UITabForm>
    <label>
      <connectCMIS>Connect CMIS Server</connectCMIS>
    </label>
  </UITabForm>
  <!-- For menu caption in Action Bar -->
  <UIActionBar>
    <tooltip>
      <ConnectCMIS>Connect CMIS Server</ConnectCMIS>
    </tooltip>
  </UIActionBar>
  <!-- For login form -->
  <CMISLoginController>
    <cmis>
      <title>Login CMIS repository</title>
      <authHelp>Authenticate to select CMIS repository for login.</authHelp>
      <loginHelp>Select CMIS repository and complete login.</loginHelp>
    </cmis>
    <serviceUrl>Service URL</serviceUrl>
    <serviceUrlHint>Server URL...</serviceUrlHint>
    <username>User name</username>
    <usernameHint>Your user name...</usernameHint>
    <password>Password</password>
    <passwordHint>Your password...</passwordHint>
    <authenticate>Authenticate</authenticate>
    <repository>CMIS repository</repository>
    <login>Login</login>
    <cancel>Cancel</cancel>
  </CMISLoginController>
</bundle>